//without argument and with return type
#include <stdio.h>
#include <math.h>
int getNumberFromUser();
int calculateSquareRoot(int number);
int main() {
int inputNumber = getNumberFromUser();
int result = calculateSquareRoot(inputNumber);
printf("The square root of %d is %d\n", inputNumber, result);
return 0;
}
int getNumberFromUser() {
int num;
printf("Enter a number: ");
scanf("%d", &num);
return num;
}
int calculateSquareRoot(int number) {
int result = sqrt(number);
return result;
}

